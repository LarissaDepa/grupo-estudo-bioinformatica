'''SEGUNDA FORMA'''


file_name = input('Nome do arquivo')
file_name_result = file_name.replace('txt', 'result')

with open(file_name) as data:
    nucleotideos_DNA = data.read()
    nucleotideos_DNA.upper()
    nucleotideos_DNA = nucleotideos_DNA.replace('T', 'u').upper()
    print(nucleotideos_DNA)
